{
	"extends": "./node_modules/@dzeio/config/tsconfig.nextjs.json",
	"compilerOptions": {
		"target": "esnext",
		"module": "esnext",
		"baseUrl": "./src",
		"paths": {
			"@styl/*": [
				"styl/*"
			],
			"@cp/*": [
				"components/*"
			],
		}
	},
	"exclude": [
		"node_modules",
		"out",
		"__tests__"
	],
	"include": [
		"next-env.d.ts",
		"src/styl/stylus.d.ts",
		"**/*.ts",
		"**/*.tsx"
	]
}
